{
  "name": "first-node-express-heroku-app",
  "version": "1.0.0",
  "description": "Playground app to play with Heroku and node in a live / public environment. Longer term aim to add mongo, authentication, api and isomorphic react. Based on https://github.com/heroku/node-js-getting-started",
  "main": "index.js",
  "scripts": {
    "start": "./node_modules/.bin/babel-node index.js",
    "dev": "webpack-dev-server --progress --color && npm run start",
    "build": "webpack --progress --color -p --config webpack.prod.config.js"
  },
  "dependencies": {
    "axios": "^0.5.4",
    "babel": "^5.8.23",
    "bcrypt": "^0.8.5",
    "body-parser": "^1.14.0",
    "cors": "^2.7.1",
    "express": "4.13.3",
    "express-jwt": "^3.1.0",
    "history": "^1.9.1",
    "immutable": "^3.7.5",
    "jsonwebtoken": "^5.0.5",
    "mongoose": "^4.1.7",
    "node-env-file": "^0.1.7",
    "object-assign": "^4.0.1",
    "react": "^0.13.3",
    "react-redux": "^2.1.2",
    "react-router": "1.0.0-rc1",
    "redux": "^3.0.0",
    "redux-thunk": "^1.0.0"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.2",
    "babel-loader": "^5.3.2",
    "react-hot-loader": "^1.3.0",
    "webpack": "^1.12.1",
    "webpack-dev-server": "^1.11.0"
